<Window x:Class="GroundhogDesktop.Views.Backups.BackupsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Style="{DynamicResource ResourceKey='Window style'}"
        Title="{DynamicResource ResourceKey=Lang.Backup.Backup}"
        MinHeight="410" MinWidth="611" Height="410" Width="611">
    <Grid>
        <TabControl Name="tc" Grid.Row="1">
            <TabItem Header="{DynamicResource ResourceKey=Lang.Backup.Local}">
                <Frame Name="fLocalBackups"/>
            </TabItem>
            <TabItem Header="{DynamicResource ResourceKey=Lang.Backup.Cloud}">
                <Frame Name="fCloudBackups"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
